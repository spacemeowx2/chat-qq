{
	"name": "chat-qq",
	"displayName": "chat-qq",
	"description": "Chat with your QQ friends with https://cqhttp.cc/",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.chatqq.setserver",
		"onView:chatqq-treeview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [{
				"id": "chatqq-treeview",
				"title": "Chat QQ",
				"icon": "media/qq.svg"
			}]
		},
		"views": {
		  "chatqq-treeview": [
				{
					"id": "chatqq-treeview",
					"name": "Chat QQ"
				}
		  ]
		},
		"commands": [
			{
				"command": "extension.chatqq.setserver",
				"title": "ChatQQ: Set server"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
